{
  "dev": {
    "driver": "mysql",
    "host": "localhost",
    "user": { "ENV": "DB_USER" },
    "password": { "ENV": "DB_PASS" },
    "database": "database-name",
    "multipleStatements": true
  },
  "production": {
    "driver": "root",
    "user": { "ENV": "DB_USER_NAME" },
    "password": { "ENV": "DB_PASSWORD" },
    "host": { "ENV": "DB_HOST" },
    "database": { "ENV": "DB_NAME" },
    "port": { "ENV": "DB_PORT" }
  }
}
